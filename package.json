{
  "name": "jmaple",
  "version": "1.0.0-alpaha.1",
  "description": "its a library to show a conversation window on your website",
  "main": "lib/index.js",
  "types": "./types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ertrii/JMaple"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "tsc ./src/index.ts --outDir ./lib && node ./lib/index.js",
    "start": "nodemon",
    "lint": "eslint . --ext .ts",
    "types": "tsc --d --declarationDir ./types",
    "publish:lib": "npm run lint && npm run test && npm run types && npm run tsc && npm publish"
  },
  "files": [
    "lib",
    "types"
  ],
  "keywords": [
    "conversation",
    "window",
    "maplestory",
    "scripts",
    "npc"
  ],
  "bugs": "https://github.com/ertrii/JMaple/issues",
  "author": "Erick(ertrii) <ertriilive@gmail.com> (https://github.com/ertrii)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.16",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "express": "^4.17.1",
    "pug": "^3.0.2"
  }
}
