{
  "name": "jmaple",
  "version": "1.0.0-alpaha.1",
  "description": "its a library to show a conversation window on your website",
  "main": "lib/index.js",
  "types": "./types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ertrii/JMaple"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "dev": "tsc --d --outDir ./lib && node ./lib/index.js",
    "start": "nodemon",
    "lint": "eslint . --ext .ts",
    "types": "tsc --d --declarationDir ./types",
    "publish:lib": "npm run lint && npm run test && npm run types && npm run tsc && npm publish"
  },
  "files": [
    "lib",
    "types"
  ],
  "keywords": [
    "conversation",
    "window",
    "maplestory",
    "scripts",
    "npc"
  ],
  "bugs": "https://github.com/ertrii/JMaple/issues",
  "author": "Erick(ertrii) <ertriilive@gmail.com> (https://github.com/ertrii)",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.3",
    "@types/node": "^14.18.21",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.4.1",
    "jest": "^28.1.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "express": "^4.18.1",
    "pug": "^3.0.2"
  }
}
